{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~assets": ["assets"],
      "~screens": ["src/screens"],
      "~navigation": ["src/navigation"],
      "~types": ["src/types"],
      "~services": ["src/services"],
      "~theme": ["src/theme"],
      "~components": ["src/components"],
      "~redux": ["src/redux"]
    },
  },
  "include": ["**/*.ts", "**/*.tsx"],
}
